{
  "name": "ymshop-uniapp",
  "version": "1.0.0",
  "description": "A modern e-commerce mobile application built with uni-app framework, featuring Vue 3, TypeScript, and Pinia for cross-platform development.",
  "main": "src/main.js",
  "scripts": {
    "dev": "uni -p mp-weixin",
    "dev:h5": "uni",
    "dev:app": "uni -p app",
    "dev:mp-alipay": "uni -p mp-alipay",
    "build": "uni build -p mp-weixin",
    "build:h5": "uni build",
    "build:app": "uni build -p app",
    "build:mp-alipay": "uni build -p mp-alipay",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .js,.ts,.vue --fix",
    "format": "prettier --write src/**/*.{js,ts,vue,json,scss,css}",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inlmcn/ymshop-uniapp.git"
  },
  "keywords": [
    "uni-app",
    "vue3",
    "typescript",
    "ecommerce",
    "mobile-app",
    "cross-platform"
  ],
  "author": "inlmcn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inlmcn/ymshop-uniapp/issues"
  },
  "homepage": "https://github.com/inlmcn/ymshop-uniapp#readme",
  "dependencies": {
    "@vant/area-data": "1.5.0",
    "add": "^2.0.6",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.10",
    "flyio": "^0.0.4",
    "html5-qrcode": "^2.3.8",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.6",
    "qrcode": "^1.5.4",
    "qs": "^6.11.2",
    "side-channel": "^1.1.0",
    "vconsole": "^3.15.1",
    "vue": "^3.3.7"
  },
  "devDependencies": {
    "@dcloudio/uni-app": "3.0.0-4080720251210001",
    "@dcloudio/uni-automator": "3.0.0-4080720251210001",
    "@dcloudio/uni-cli-shared": "3.0.0-4080720251210001",
    "@dcloudio/vite-plugin-uni": "3.0.0-4080720251210001",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.14.1",
    "prettier": "^3.0.0",
    "sass": "1.57.1",
    "sass-loader": "13.2.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "vite": "^4.5.14",
    "vitest": "^0.32.0"
  }
}
